<% content_for(:title, 'CV Sebastian Graz') %>

<%= partial "partials/nav", :locals => {:textColor => '#000' } %>

<div class="cv row">
  <div class=" typography db flex-ns flex-row-ns flex-column justify-between">
    <% data.site.cvTemplate.each do | id, this | %>


      <main class="measure-wide fl mt5 mt6-ns pr0 pr4-ns ">
        <%= markdown(this.cvIntro) %>
        <ul>
          <%  this.cvExperiences.each do | this | %>
            <li class="mv5">
            <p><%= this.title %></p>
            <% this.multicontentContainer.each do | this | %>
              <%= markdown(this.textBlock) %>
            <% end %>
            </li>
          <% end %>
        </ul>
      </main>


      <aside class="w-100 w-30-ns fl ">
        <ul class="mv5 mv6-ns">
        <%  this.cvDetails.each do | this | %>
          <li class="dib w-100">
          <p><%= this.title %></p>
          <% this.multicontentContainer.each do | this | %>
            <div class="di">
               <%= markdown(this.textBlock) %>
            </div>
          <% end %>
          </li>
        <% end %>
        </ul>
      </aside>


    <% end %>
  </div>
</div>

<!doctype html>
<html>
<head>
  <meta content="IE=edge" http-equiv="X-UA-Compatible">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">

  <link href="/images/favicon/favicon-32x32.png" rel="icon" sizes="32x32" type="image/png"></link>
  <link href="/images/favicon/favicon-16x16.png" rel="icon" sizes="16x16" type="image/png"></link>

  <!-- Use title if it's in the page YAML frontmatter -->
  <title><%= yield_content(:title) %></title>

<link rel="stylesheet" href="//cdn.jsdelivr.net/highlight.js/9.11.0/styles/monokai-sublime.min.css">
<script src="//cdn.jsdelivr.net/highlight.js/9.11.0/highlight.min.js"></script>

  <%= stylesheet_link_tag :tachyons %>
  <%= stylesheet_link_tag :site %>

</head>

<body id="wrapper">
  <%= yield %>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-82393962-1', 'auto');
    ga('send', 'pageview');

  </script>
  <%= javascript_include_tag :jquery %>
  <%= javascript_include_tag :barba %>
  <%= javascript_include_tag :all %>
  <%= javascript_include_tag :scrollreveal %>
  <%= javascript_include_tag :videoreveal %>

  <%= javascript_include_tag :cookie %>
  <%= javascript_include_tag :gibberish %>
</body>
</html>
